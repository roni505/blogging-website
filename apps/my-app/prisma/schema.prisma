

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id @default(uuid())
  email     String @unique
  name      String?
  password  Int
  post      Post[]
}

model Post {
  id        String @id @default(uuid())
  author_id String
  author    User @relation(fields: [author_id], references: [id])
  title     String
  content   String
}
